{
  "name": "@alice-finance/exchange",
  "version": "1.1.0-beta.1",
  "description": "Solidity contracts for the exchange on alice network",
  "files": [
    "abis",
    "contracts",
    "test",
    ".openzeppelin"
  ],
  "scripts": {
    "compile": "./node_modules/.bin/truffle compile",
    "build": "./node_modules/.bin/truffle compile --all && node scripts/build.js",
    "test": "./scripts/test.sh",
    "test:coverage": "./node_modules/.bin/solidity-coverage",
    "deploy": "scripts/deploy.sh",
    "upgrade": "scripts/upgrade.sh",
    "prettier": "./node_modules/.bin/prettier contracts/**/*.sol --write ",
    "prettier:js": "./node_modules/.bin/prettier test/**/*.js --write --print-width 120"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alice-finance/exchange.git"
  },
  "author": "Changhwan Choi <changhwan@dnext.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alice-finance/exchange/issues"
  },
  "homepage": "https://github.com/alice-finance/exchange",
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^2.2.3",
    "@openzeppelin/upgrades": "^2.5.3",
    "openzeppelin-solidity": "^2.3.0"
  },
  "devDependencies": {
    "@alice-finance/oz-cli": "^2.5.3",
    "@codechecks/client": "^0.1.10",
    "bn.js": "^4.11.0",
    "chai": "^4.2.0",
    "chai-bn": "^0.2.0",
    "dotenv": "^8.0.0",
    "eth-gas-reporter": "^0.2.0",
    "ganache-cli": "^6.7.0",
    "loom-js": "^1.51.1",
    "loom-truffle-provider": "^0.13.0",
    "openzeppelin-test-helpers": "^0.4.0",
    "prettier": "^1.18.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.34",
    "solhint": "^2.0.0",
    "solidity-coverage": "^0.6.0",
    "truffle": "^5.0.2",
    "yargs": "12.0.5"
  }
}
